{
    "manifest_version": 2,
    "name": "SteamAnalyst Lookup - CSGO",
    "short_name" : "SteamAnalyst+ and Lookup",
    "version": "0.0.4",
    "content_scripts": [{
        "matches": [
            "*://csgolounge.com/*",
            "*://csgotraders.net/*",
            "*://csgo.steamanalyst.com/*"
        ],
        "js": ["weapons.js", "jquery.js", "content.js"]
    }],
    "background": {
        "scripts": ["jquery.js", "background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon.png"
    }
}
